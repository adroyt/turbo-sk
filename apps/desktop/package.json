    "format": "prettier --write --ignore-path=../../.prettierignore ."
  },
  "devDependencies": {
    "@packages/config": "workspace:*",
    "@sveltejs/kit": "1.0.1",
    "@unocss/reset": "^0.48.0",
    "eslint-config-custom": "workspace:*",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.1",
    "vite": "^4.0.3"
  },
  "dependencies": {
    "clsx": "^1.2.1"
  }
}
